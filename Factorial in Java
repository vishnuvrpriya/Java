import java.util.Scanner;
class Factorial
{
public static void main(String[]args)
{
int res = 1;
int i;
int n;
Scanner s = new Scanner(System.in);
System.out.println("Enter the value to which factorial has to be found");
n = s.nextInt();
for(i = 1; i<=n; i++)
{
res=res*i;
}
System.out.println("Factorial is : " +res);
}
}
